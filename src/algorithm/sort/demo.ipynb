    "from typing import List\n",
    "\n",
    "\n",
    "class Solution:\n",
    "    def findKthLargest(self, nums: List[int], k: int) -> int:\n",
    "        sorted_nums = sorted(nums, reverse=True)\n",
    "        return sorted_nums[:k]\n",
    "\n",
    "\n",
    "res = Solution().findKthLargest([3, 2, 1, 5, 6, 4], 2)\n",
    "print(res)"
   ]
